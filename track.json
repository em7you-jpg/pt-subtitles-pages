{
  "title": "しなやかさで歩く日々",
  "lines": [
    {
      "start": 0.68,
      "end": 0.68,
      "ja": "",
      "ko_pron": "",
      "ko": "",
      "tag": "Intro - instrumental"
    },
    {
      "start": 0.0,
      "end": 5.12,
      "ja": "目覚まし鳴って伸びる腕　カーテン越しの光探して",
      "ko_pron": "",
      "ko": "",
      "tag": "Verse 1"
    },
    {
      "start": 5.12,
      "end": 10.88,
      "ja": "忙しい朝の支度　昨日の迷いは置いていく",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 3"
    },
    {
      "start": 10.88,
      "end": 15.92,
      "ja": "新しい靴じゃなくても　足元に響くリズムは同じ",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 4"
    },
    {
      "start": 15.92,
      "end": 22.2,
      "ja": "歩き始めた交差点　胸の奥で微かな合図",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 5"
    },
    {
      "start": 22.2,
      "end": 27.44,
      "ja": "失敗しても帰れない　決めた道だから進むだけ",
      "ko_pron": "",
      "ko": "",
      "tag": "Pre-Chorus"
    },
    {
      "start": 27.44,
      "end": 34.64,
      "ja": "心がしなやかだと感じた瞬間　私は強くなれる",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 7"
    },
    {
      "start": 34.64,
      "end": 40.44,
      "ja": "目の前の段差も飛び越えて　自分に負けないように",
      "ko_pron": "",
      "ko": "",
      "tag": "Chorus"
    },
    {
      "start": 40.44,
      "end": 56.9,
      "ja": "小さな勇気を集めながら　今日も進んでいく",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 9"
    },
    {
      "start": 56.9,
      "end": 62.12,
      "ja": "友達の笑顔思い出す　背筋伸ばして深呼吸",
      "ko_pron": "",
      "ko": "",
      "tag": "Verse 2"
    },
    {
      "start": 62.12,
      "end": 67.32,
      "ja": "疲れた昨日のページを　カバンの奥に閉じ込めて",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 11"
    },
    {
      "start": 67.32,
      "end": 73.66,
      "ja": "忘れ物が増えても　気持ちだけは置いていかない",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 12"
    },
    {
      "start": 73.66,
      "end": 79.14,
      "ja": "あの日から続くストーリー　進化する自分確かめる",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 13"
    },
    {
      "start": 79.14,
      "end": 86.18,
      "ja": "失敗しても帰れない　決めた道だから進むだけ",
      "ko_pron": "",
      "ko": "",
      "tag": "Pre-Chorus"
    },
    {
      "start": 86.18,
      "end": 91.4,
      "ja": "心がしなやかだと感じた瞬間　私は強くなれる",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 15"
    },
    {
      "start": 91.86,
      "end": 117.46,
      "ja": "目の前の段差も飛び越えて　自分に負けないように",
      "ko_pron": "",
      "ko": "",
      "tag": "Chorus"
    },
    {
      "start": 117.46,
      "end": 123.56,
      "ja": "小さな勇気を集めながら　今日も進んでいく",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 17"
    },
    {
      "start": 123.56,
      "end": 130.4,
      "ja": "昨日の涙も今は乾いて　靴下の跡が消えるころ",
      "ko_pron": "",
      "ko": "",
      "tag": "Bridge"
    },
    {
      "start": 130.4,
      "end": 140.58,
      "ja": "新しい自分に会える気がして　振り返らずに歩く",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 19"
    },
    {
      "start": 140.58,
      "end": 146.18,
      "ja": "目の前の段差も飛び越えて　自分に負けないように",
      "ko_pron": "",
      "ko": "",
      "tag": "Final Chorus"
    },
    {
      "start": 146.18,
      "end": 151.22,
      "ja": "小さな勇気を集めながら　今日も進んでいく",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 21"
    },
    {
      "start": 151.22,
      "end": 157.12,
      "ja": "前より強くなれた気がして　笑顔でまた踏み出す",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 22"
    },
    {
      "start": 157.12,
      "end": 173.04,
      "ja": "夕暮れ染まる通り抜けて　明日も私はしなやかに",
      "ko_pron": "",
      "ko": "",
      "tag": "Outro"
    }
  ]
}