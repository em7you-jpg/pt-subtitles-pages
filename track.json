{
  "title": "止まらない静かな歩み",
  "lines": [
    {
      "start": 13.22,
      "end": 13.22,
      "ja": "",
      "ko_pron": "",
      "ko": "",
      "tag": "Intro - instrumental"
    },
    {
      "start": 13.22,
      "end": 19.28,
      "ja": "今日は小さな決心　遅い街の角を曲がる",
      "ko_pron": "",
      "ko": "",
      "tag": "Verse 1"
    },
    {
      "start": 19.28,
      "end": 24.38,
      "ja": "手にしたノートの端　昨日よりも遠くへ行く",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 3"
    },
    {
      "start": 24.38,
      "end": 30.64,
      "ja": "ゆっくりでいいと自分に言い聞かせた",
      "ko_pron": "",
      "ko": "",
      "tag": "Pre-Chorus"
    },
    {
      "start": 30.64,
      "end": 38.42,
      "ja": "立ち止まらず足元だけを見つめて進む",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 5"
    },
    {
      "start": 38.42,
      "end": 43.52,
      "ja": "目印のない道でも　一歩ずつ描いていく",
      "ko_pron": "",
      "ko": "",
      "tag": "Chorus"
    },
    {
      "start": 43.52,
      "end": 61.26,
      "ja": "途中で見つけた石ころ　そっと蹴って未来へ投げる",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 7"
    },
    {
      "start": 61.26,
      "end": 65.76,
      "ja": "伸びた影が重なる　通り過ぎた自販機の光",
      "ko_pron": "",
      "ko": "",
      "tag": "Verse 2"
    },
    {
      "start": 65.76,
      "end": 72.42,
      "ja": "ポケットの中で丸めたメモ　新しい空気を吸い込む",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 9"
    },
    {
      "start": 72.42,
      "end": 77.58,
      "ja": "ゆっくりでいいと自分に言い聞かせた",
      "ko_pron": "",
      "ko": "",
      "tag": "Pre-Chorus"
    },
    {
      "start": 77.58,
      "end": 83.76,
      "ja": "立ち止まらず足元だけを見つめて進む",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 11"
    },
    {
      "start": 83.76,
      "end": 91.04,
      "ja": "目印のない道でも　一歩ずつ描いていく",
      "ko_pron": "",
      "ko": "",
      "tag": "Chorus"
    },
    {
      "start": 91.04,
      "end": 96.6,
      "ja": "途中で見つけた石ころ　そっと蹴って未来へ投げる",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 13"
    },
    {
      "start": 96.6,
      "end": 113.63,
      "ja": "昨日の迷いを置いていく　振り返らずに進むことを覚える",
      "ko_pron": "",
      "ko": "",
      "tag": "Bridge"
    },
    {
      "start": 113.63,
      "end": 118.29,
      "ja": "冷たい手すり握りしめて　朝焼け前の空気を信じた",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 15"
    },
    {
      "start": 118.89,
      "end": 126.65,
      "ja": "目印のない道でも　一歩ずつ描いていく",
      "ko_pron": "",
      "ko": "",
      "tag": "Final Chorus"
    },
    {
      "start": 126.65,
      "end": 135.31,
      "ja": "途中で見つけた石ころ　そっと蹴って未来へ投げる",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 17"
    },
    {
      "start": 135.31,
      "end": 140.91,
      "ja": "何も決まっていないけど　歩みを止めたくない",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 18"
    },
    {
      "start": 140.91,
      "end": 148.71,
      "ja": "静かな心で今日を越えていきたい",
      "ko_pron": "",
      "ko": "",
      "tag": "Line 19"
    },
    {
      "start": 148.71,
      "end": 167.57,
      "ja": "靴音が遠ざかる　余韻だけが残る帰り道",
      "ko_pron": "",
      "ko": "",
      "tag": "Outro"
    }
  ]
}